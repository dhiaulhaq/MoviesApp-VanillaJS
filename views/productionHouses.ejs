<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Movies App</title>
</head>
<body>
    <%- include('_nav') %>
    <h1>Production Houses</h1>
    <table border="1">
        <thead>
            <tr>
                <th>Name</th>
                <th>Headquarters</th>
            </tr>
        </thead>
        <tbody>
            <% productionHouses.forEach(el => { %>
             <tr>
                <td>
                    <%= el.name_prodHouse %>
                </td>
                <td>
                    <%= el.headquarters %>
                </td>
             </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>